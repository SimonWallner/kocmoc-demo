@article{BiriMichelinArqus03Real-Time-Single,
	Abstract = {The representation of participating media adds a very realistic effect in computer generated images. Unfortunately, they are especially difficult to render. Indeed, their contribution in the illumination of a scene is quite complex, depending on both light position and camera position. We present here a new algorithm that is able to render, in real-time, the single scattering induced by any participating medium. Based on a new expression of the radiative transfer equation, our algorithm uses shadow planes to compute efficiently the participating medium contribution along rays linking the eye and the scene. It is also able to manage efficiently not only the surface shadows, but also the volumetric shadows casted in such medium, and this, without aliasing. This method, quite simple, can be used to compute quickly animations that represent scenes covered by a homogeneous participating medium.},
	Affiliation = {Gaspard Monge Institute , University of Marne-la-Valle},
	Author = {V Biri and S Michelin and D Arqus},
