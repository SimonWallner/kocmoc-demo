  language = {English},
  local-url = {file://localhost/Users/Simon/Daten/Studium/Papers/2008/Bruneton/Comput%20Graph%20Forum%202008%20BrunetonPrecomputed%20atmospheric%20scattering.pdf},
  month = {Jan},
